//##+----------+------------------+-------+---------------+------+----------+##
//##|Descr.    | Método para avançar um Node no caminho xPath do XML sem    |##
//##|          |  precisar passar o caminho completo                        |##
//##+----------+---------------+--------------------------------------------+##
//##|   DATA   | Programador   | Manutenção efetuada                        |##
//##+----------+---------------+--------------------------------------------+##
//##|          |               |                                            |##
//##|          |               |                                            |##
//##+----------+---------------+--------------------------------------------+##
//#############################################################################
Method CXxPathChildNode(cXSubPathExpr	,;
						lOpcional		;	//Não considera como erro se não achar o nó, para não alimentar as varíaveis de erro
								) Class tCXXMLManager

	//Parâmetros da rotina-------------------------------------------------------------------------
	ParamType 0		VAR cXSubPathExpr		AS Character
	ParamType 1		VAR lOpcional			AS Logical		Optional Default .F.
	
	::ResetErrors()	//Limpo as mensagens de erro

Return ::CXSetXPath(::_xPath+cXSubPathExpr	,;
					lOpcional				)

//#############################################################################
//##+----------+-------------------+-------+--------------+------+----------+##
//##|Programa  | CXxPathParentNode | Autor | Cirilo Rocha | Data |10/06/2022|##
//##+----------+-------------------+-------+--------------+------+----------+##
//##|Descr.    | Método para retroceder um Node no caminho xPath do XML sem |##
//##|          |  precisar passar o caminho completo                        |##
//##+----------+---------------+--------------------------------------------+##
//##|   DATA   | Programador   | Manutenção efetuada                        |##
//##+----------+---------------+--------------------------------------------+##
//##|          |               |                                            |##
//##|          |               |                                            |##
//##+----------+---------------+--------------------------------------------+##
//#############################################################################
Method CXxPathParentNode() Class tCXXMLManager

	//Declaração de Variáveis----------------------------------------------------------------------
	Local nPos		AS Integer
	Local lRet		AS Logical
	
	//---------------------------------------------------------------------------------------------
	::ResetErrors()	//Limpo as mensagens de erro
	
	If Empty(::_xPath)
		lRet		:= .F.
		
		::_cCodErr	:= U_CXTxtMsg(Self,,.T.)+'-'+U_CXTxtMsg(,2,.T.)
		::_cMsgErr	:= 	'ERRO ao voltar no XPath. Está vazio.'
		::_cSoluc	:= ''
		::_cError	:= ::_cCodErr+': '+::_cMsgErr+IIf(!Empty(::_cSoluc),CRLF+'SOLUÇÃO: '+::_cSoluc,'')

	Else
		nPos	:= Rat('/',::_xPath)
		If nPos == 0
			lRet		:= .F.
			
			::_cCodErr	:= U_CXTxtMsg(Self,,.T.)+'-'+U_CXTxtMsg(,2,.T.)
			::_cMsgErr	:= 	'ERRO ao voltar no XPath: '+::_xPath+CRLF+;
							'Não localizou o Nó anterior.'
			::_cSoluc	:= ''
			::_cError	:= ::_cCodErr+': '+::_cMsgErr+IIf(!Empty(::_cSoluc),CRLF+'SOLUÇÃO: '+::_cSoluc,'')

		ElseIf nPos <= 3
			lRet		:= .F.
			
			::_cCodErr	:= U_CXTxtMsg(Self,,.T.)+'-'+U_CXTxtMsg(,2,.T.)
			::_cMsgErr	:= 	'ERRO ao setar XPath: '+::_xPath+CRLF+;
							'Este caminho não existe.'
			::_cSoluc	:= ''
			::_cError	:= ::_cCodErr+': '+::_cMsgErr+IIf(!Empty(::_cSoluc),CRLF+'SOLUÇÃO: '+::_cSoluc,'')

		Else
			lRet		:= ::CXSetXPath(Left(::_xPath,nPos-1))
		EndIf
	EndIf
	
	If .Not. lRet
		//Alimento as variáveis compartilhadas
		cCodErr	:= ::_cCodErr
		cMsgErr	:= ::_cMsgErr
		cSoluc	:= ::_cSoluc
		cMsg	:= ::_cError
	EndIf

Return lRet

