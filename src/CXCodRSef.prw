Static aCodRSef		:= __initCod()
Static aCodRSefMDF	:= __initMDFe()
//-------------------------------------------------------------------------------------------------
User Function CXCodRSef(cCodSef	,;	//01
						cModelo	);	//02
							AS Character

	//Declaracao de variaveis----------------------------------------------------------------------
	Local nPos			AS Numeric
	Local cRet			AS Character

	//Parametros da rotina-------------------------------------------------------------------------
	ParamType 0		VAR cCodSef  	  		AS Character
	ParamType 1		VAR cModelo  	  		AS Character	Optional Default '55'
	
	//---------------------------------------------------------------------------------------------
	If cModelo == '58'	//MDF-e 
		nPos	:= aScan(aCodRSefMDF,{|x| x[1] == cCodSef })
		
		If nPos == 0
			cRet	:= U_CXTxtMsg()+'Código de retorno da Sefaz '+cCodSef+' não mapeado.'
		Else
			cRet	:= aCodRSefMDF[nPos][2]
		EndIf
	Else
		nPos	:= aScan(aCodRSef,{|x| x[1] == cCodSef })
		
		If nPos == 0
			cRet	:= U_CXTxtMsg()+'Código de retorno da Sefaz '+cCodSef+' não mapeado.'
		Else
			cRet	:= aCodRSef[nPos][2]
		EndIf
	EndIf
	
Return cRet

