User Function CXaDel(	aDados	,;
						nPos	);
							AS Array

	//Parametros da rotina-------------------------------------------------------------------------
	ParamType 0		VAR aDados  	  		AS Array
	ParamType 1		VAR nPos		  	  	AS Numeric
	
	//---------------------------------------------------------------------------------------------
	aDel(@aDados,nPos)
	aSize(@aDados,len(aDados)-1)

Return aDados
